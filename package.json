{
  "name": "xkanban-api",
  "version": "1.0.0",
  "description": "API for xkanban client",
  "main": "index.ts",
  "author": "Nikolay Rademacher",
  "license": "MIT",
  "private": true,
  "scripts": {
    "generate": "graphql-codegen",
    "prismix": "prismix && prisma format",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "DATABASE_HOST=localhost prisma migrate dev",
    "prisma": "yarn prismix && yarn prisma:generate && yarn prisma:migrate",
    "prisma:studio": "DATABASE_HOST=localhost prisma studio",
    "dev": "DATABASE_HOST=localhost concurrently \"ts-node-dev --no-notify --respawn --transpile-only\" \"yarn generate  --watch\"",
    "build": "tsc",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@types/node": "^16.9.4",
    "concurrently": "^6.2.2",
    "prettier": "^2.4.1",
    "prisma": "3.0.2",
    "prismix": "^1.0.19",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-resolvers": "^2.2.1",
    "@graphql-tools/graphql-file-loader": "^7.1.0",
    "@graphql-tools/load": "^7.3.0",
    "@prisma/client": "3.0.2",
    "apollo-server": "^3.3.0",
    "dotenv": "^10.0.0",
    "graphql": "^15.6.0",
    "lodash": "^4.17.21"
  }
}
