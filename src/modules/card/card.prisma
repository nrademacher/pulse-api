model Card {
  id         String @id @default(cuid())
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  title      String   
  content    String?
  list       List @relation(fields: [listId], references: [id])
  listId     String

  @@map("cards")
}

model List {
  id     String @id @default(cuid())
  cards  Card[]
}
